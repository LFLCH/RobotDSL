{
  "compilerOptions": {
    "target": "ES2017",
    "module": "Node16",
    "lib": ["ESNext", "DOM", "WebWorker"],
    "sourceMap": true,
    "outDir": "out",
    "strict": true,
    "noUnusedLocals": false,
    "noImplicitReturns": true,
    "noImplicitOverride": true,
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
     "src/**/*.ts", "src/static/scripts/setup.ts",
    //  "src/static/scripts/rbs-canvas.ts"
  ],
  "exclude": [
     "out",
     "node_modules"
  ]
}
